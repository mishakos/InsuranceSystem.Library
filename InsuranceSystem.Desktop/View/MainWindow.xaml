<Window x:Class="InsuranceSystem.Desktop.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InsuranceSystem.Desktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="21"/>
            <RowDefinition Height="151*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">
            <MenuItem Header="File">
                <MenuItem x:Name="mnuItemClose" Header="Close" Click="mnuItemClose_Click"/>
            </MenuItem>
            <MenuItem Header="Catalogs">
                <MenuItem x:Name="mnuItemBlanks" Header="Blanks" Click="mnuItemBlanks_Click"/>
                <MenuItem x:Name="mnuItemClients" Header="Clients" Click="mnuItemClients_Click"/>
            </MenuItem>
        </Menu>
        <StackPanel x:Name="mainPanel" Grid.Row="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            
        </StackPanel> 
    </Grid>
</Window>
