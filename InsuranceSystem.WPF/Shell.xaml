<Window x:Class="InsuranceSystem.WPF.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InsuranceSystem.WPF"
        xmlns:prism ="http://prismlibrary.com/"
        xmlns:inf ="clr-namespace:InsuranceSystem.Infrastructure;assembly=InsuranceSystem.Infrastructure"
        mc:Ignorable="d"
        Title="Insurance System">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" prism:RegionManager.RegionName="{x:Static inf:RegionNames.ToolbarRegion}"/>
        <ContentControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.ContentRegion}" />
    </DockPanel>
</Window>
